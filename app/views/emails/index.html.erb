<table id="summary">
	<tr>
		<td>Total number of emails sent</td>
		<td><%= count_event('send') %></td>
	</tr>
	<tr>
		<td>Total number of emails opened</td>
		<td><%= count_event('open') %></td>
	</tr>
	<tr>
		<td>Total number of clicks</td>
		<td><%= count_event('click') %></td>
	</tr>
	<tr>
		<td>Open rate per email type</td>
		<td>
      <ul>
        <% @email_types.each do |type| %>
        <li>
          <strong><%= type %>:</strong>
          <%= percentage_rate(type, 'open') %>
        </li>
        <% end %>
      </ul>
    </td>
	</tr>
	<tr>
		<td>Click rate per email type</td>
		<td>
      <ul>
        <% @email_types.each do |type| %>
        <li>
          <strong><%= type %>:</strong>
          <%= percentage_rate(type, 'click') %>
        </li>
        <% end %>
      </ul>
    </td>
	</tr>
</table>
